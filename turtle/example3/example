import turtle

turtle.penup()
turtle.goto(-300,0)
turtle.pendown()
turtle.forward(15)

turtle.penup()
turtle.forward(15)

turtle.pendown()
turtle.forward(15)

turtle.penup()
turtle.forward(15)

turtle.pendown()
turtle.forward(15)

turtle.penup()
turtle.forward(15)

turtle.penup()
turtle.forward(15)

turtle.done()